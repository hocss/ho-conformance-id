  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hocss/ho-conformance-id/issues"
  },
  "homepage": "https://github.com/hocss/ho-conformance-id",
  "dependencies": {
    "chalk": "^1.0.0",
    "core-js": "^0.8.3",
    "ho-conformance-events": "^0.2.0",
    "ho-conformance-base": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^5.1.9",
    "tape": "^4.0.0"
  }
}
